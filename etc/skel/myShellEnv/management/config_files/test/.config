#
# Configurações gerais
[general]
ENVIRONMENT                 =   LCL
GIT_LOG_LENGTH              =   10
ADMINISTRATOR_NAME          =   Rianna Cantarelli
ADMINISTRATOR_EMAIL         =   rianna@aeondigital.com.br
DATETIME_LOCAL              =   America/Sao_Paulo


#
# Servidor Web
[webserver]
CONTAINER_WEBSERVER_NAME    =   dev-php-webserver
APACHE_RUN_USER             =   [[apacheuser]]
APACHE_RUN_GROUP            =   [[apachegroup]]

FORCE_HTTPS                 =   0
DEBUG_MODE                  =   1
UPDATE_ROUTES               =   1

REQUEST_TIMEOUT             =   1200
REQUEST_MAX_FILESIZE        =   100
REQUEST_MAX_POSTSIZE        =   100



#
# Envio de emails
[email]
EMAIL_HOST                  =   192.168.1.206
EMAIL_PORT                  =   25

EMAIL_SMTP_SECURE           =   0
EMAIL_USE_SMTP              =   0

EMAIL_USE_AUTHENTICATION    =   0
EMAIL_AUTHENTICATION_USER   =
EMAIL_AUTHENTICATION_PASS   =
EMAIL_DEFAULT_FROM_EMAIL    =   engarde@dna.com.br



#
# Banco de Dados
[database]
CONTAINER_DBSERVER_NAME     =   dev-php-dbserver

DATABASE_TYPE               =   [[dbtype]]
DATABASE_HOST               =   [[dbhost]]
DATABASE_NAME               =   [[dbname]]
DATABASE_USER               =   [[dbuser]]
DATABASE_PASS               =   [[dbpass]]
#COMMENT_TEST_NOT            =   value
#COMMENT_TEST                =   value

#
# Teste
[teste]
GIT_LOG_LENGTH              =   20
#COMMENT_TEST                =   value
